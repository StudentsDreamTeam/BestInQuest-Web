rsion":3,"sources":["C:/BestInQuest-Web/src/features/tasks/components/TaskList.jsx"],"sourcesContent":["import Task from './Task';\r\nimport { styled } from 'styled-components';\r\nimport { useTasks } from '../../../contexts/TasksContext';\r\n\r\nconst TaskListComponent = styled.div`\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n  margin-top: 2rem;\r\n  overflow-y: auto;\r\n  padding-bottom: 1rem;\r\n`;\r\n\r\nconst Message = styled.p`\r\n  text-align: center;\r\n  font-size: 1.2rem;\r\n  color: #555;\r\n  margin-top: 2rem;\r\n`;\r\n\r\nexport default function TaskList({ onOpenUpdateTaskModal, onOpenDeleteConfirmModal }) {\r\n  const { tasks, isLoadingTasks, tasksError } = useTasks();\r\n\r\n  if (isLoadingTasks) {\r\n    return <Message>Загрузка задач...</Message>;\r\n  }\r\n\r\n  if (tasksError) {\r\n    return <Message>Ошибка загрузки задач: {tasksError}</Message>;\r\n  }\r\n\r\n  if (tasks.length === 0) {\r\n    return <Message>Задач пока нет.</Message>;\r\n  }\r\n\r\n  return (\r\n    <TaskListComponent>\r\n      {tasks.map((task) => (\r\n        <Task\r\n          key={task.id}\r\n          task={task}\r\n          onOpenUpdateModal={onOpenUpdateTaskModal}\r\n          onOpenDeleteConfirmModal={onOpenDeleteConfirmModal}\r\n        />\r\n      ))}\r\n    </TaskListComponent>\r\n  );\r\n}"],"mappings":"AAAA,OAAO,UAAU;AACjB,SAAS,cAAc;AACvB,SAAS,gBAAgB;AAEzB,MAAM,oBAAoB,OAAO;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASjC,MAAM,UAAU,OAAO;AAAA;AAAA;AAAA;AAAA;AAAA;AAOvB,wBAAwB,SAAS,EAAE,uBAAuB,yBAAyB,GAAG;AACpF,QAAM,EAAE,OAAO,gBAAgB,WAAW,IAAI,SAAS;AAEvD,MAAI,gBAAgB;AAClB,WAAO,oCAAC,eAAQ,mBAAiB;AAAA,EACnC;AAEA,MAAI,YAAY;AACd,WAAO,oCAAC,eAAQ,2BAAwB,UAAW;AAAA,EACrD;AAEA,MAAI,MAAM,WAAW,GAAG;AACtB,WAAO,oCAAC,eAAQ,iBAAe;AAAA,EACjC;AAEA,SACE,oCAAC,yBACE,MAAM,IAAI,CAAC,SACV;AAAA,IAAC;AAAA;AAAA,MACC,KAAK,KAAK;AAAA,MACV;AAAA,MACA,mBAAmB;AAAA,MACnB;AAAA;AAAA,EACF,CACD,CACH;AAEJ;","names":[]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"c68